<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false" scan="false">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd.MM.yyyy HH:mm:ss.SSS} %-5level [%thread]:[%logger] %msg%n</pattern>
        </encoder>
    </appender>

    <root level="WARN" additivity="true">
        <appender-ref ref="STDOUT" />
    </root>
    
    <Logger name="ch.ethz.seb.sebserver.HTTPClientBot" level="INFO" additivity="true" />
    <!--<Logger name="ch.ethz.seb.sebserver" level="DEBUG" additivity="true" /> -->
</configuration>